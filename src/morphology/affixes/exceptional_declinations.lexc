!---------------
! exceptional inflectional types, i.e. covering very few words each
!---------------
LEXICON ARGPÜKS   ! some compound words ending with püks (argpüks, vesipüks etc)
                  ! and declined not exactly like püksid (e.g. karupüksid, sukkpüksid) 
                  ! almost like TAUD, but has some less used pl forms
: TAUD ;
%+Use%/Rare:%{W%}%+te N_PL_SUBPARA ;           ! argpükste etc

LEXICON JALG   ! only jalg

: PIIM ;
%+Use%/Rare:%+e N_VOWEL_PL_SUBPARA ;      ! jalge

LEXICON KAEL   ! VVS 23  almost = PIIM, except pl gen + 

+Sg:%{no.apo%} NOMINATIVE ;
:%{sg.a%} 1C_SG ;
:%{W%}%+te N_PL_SUBPARA ;                     ! kaelte etc 
%+Use%/Rare:%{sg.a%}%+de N_PL_SUBPARA ;       ! kaelade etc 
                                              ! pl par stem vowel (u, i, 0) is lexically determined 
                                              ! and usage-wise tagged
+Pl:%{sg.a%}%+%{s%}%{i%}%{d%} PL_PAR_VARIANT ;  ! kaelu, kaelasid+Use/Rare

LEXICON KALLIS    !  VVS 7, 5 only kallis, kaunis unstable (closed) type; 

+Sg:s%{W%} NOMINATIVE ;                      ! kallis
+Sg:s%{W%}%+t PARTITIVE ;                   ! kallist
:%+te N_PL_SUBPARA ;                     ! kallite etc 
%+Use%/Rare:s%{W%}%+te N_PL_SUBPARA ;    ! kalliste etc 
: 2C_STRONG ;                            ! kalli etc, kalleid, kalleil etc

LEXICON KAKS                  ! only üks, kaks

+Sg:ks NOMINATIVE ;                                ! kaks
