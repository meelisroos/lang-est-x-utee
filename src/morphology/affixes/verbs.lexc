LEXICON IND_COND_PRS   ! indicative, conditional present + imp sg2

+Imprt+Sg2: GI ;
+Ind+Neg: GI ;
+Ind+Sg1+Aff:n GI ;
+Ind+Sg2+Aff:d GI ;
+Ind+Sg3+Aff:b GI ;
+Ind+Pl1+Aff:me GI ;
+Ind+Pl2+Aff:te GI ;
+Ind+Pl3+Aff:vad GI ;
: CONDITIONAL ;

LEXICON IND_PAST       ! indicative past
: IND_PAST1 ;
+Sg3+Aff: GI ;

LEXICON IND_PAST1       ! indicative past, except sg3

+Sg1+Aff:in GI ;
+Sg2+Aff:id GI ;
+Pl1+Aff:ime GI ;
+Pl2+Aff:ite GI ;
+Pl3+Aff:id GI ;

LEXICON NU      !  NU-affixes

: CONDITIONAL ;
! +Imprt:d GI ;      ! omitted for now because it would create homonymy
+Ind+Neg:d GI ;
+Prc:d GI ;
+Quot:vat GI ;

LEXICON IMPERATIVE      ! imperative

:u GI ;              ! elagu
+Pl1:em GI ;      ! elagem
+Pl2:e GI ;       ! elage

LEXICON SUPINE_V

+Sup:ma SUPINE ;
+Prc:v GI ;
+Quot:vat GI ;

LEXICON SUPINE

+Ill: GI ;     ! elama
+Ine:s GI ;    ! elamas
+Ela:st GI ;
+Tra:ks GI ;
+Abe:ta GI ;

LEXICON IMPERS  ! impersonal forms
: IMPERS1 ;
+Prs+Ind+Aff:akse GI ;

LEXICON IMPERS1  ! impersonal forms, except -[td]akse

+Prs+Cond:aks GI ;
+Prs+Imprt:agu GI ;
+Prs+Ind+Neg:a GI ;
+Prs+Prc:av GI ;
+Prs+Quot:avat GI ;
+Prt+Cond:uks GI ;
+Prt+Ind+Aff:i GI ;
+Prt+Ind+Neg:ud GI ;
+Prt+Prc:ud GI ;
+Sup:ama GI ;

LEXICON INFINITIVE     ! infinitive, gerund (des-form)

+Inf:a GI ;
+Ger:es GI ;

LEXICON CONDITIONAL    ! conditional present and past  

+Cond:ks GI ;
+Cond+Sg1+Aff:ksin GI ;
+Cond+Sg2+Aff:ksid GI ;
+Cond+Pl1+Aff:ksime GI ;
+Cond+Pl2+Aff:ksite GI ;
+Cond+Pl3+Aff:ksid GI ;


