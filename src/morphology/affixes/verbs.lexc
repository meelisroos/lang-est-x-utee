
!! !!!Verb inflection
! VVS stands for Ü. Viks "Väike vormisõnastik" for reference purposes

LEXICON ELAMA    ! VVS 27

                      : IMPERS ;        ! impersonal forms
                 +Pers:%+ SUPINE_V ;       ! -ma etc, -v, -vat
                      :%+D7 INFINITIVE ;    ! infinitive, gerund (des-form)
       +Pers+Prs+Imprt:%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem
             +Pers+Prt:%+nu NU ;           !  NU-affixes
         +Pers+Prt+Ind:%+I7S7 IND_PAST ;      ! indicative past
             +Pers+Prs:%+ IND_COND_PRS ;   ! 

LEXICON JULGEMA   ! VVS 27 julgema; like ELAMA, but there are parallel weak grade forms

                      : ELAMA ;    
           %+Use%/Rare:%{W%} IMPERS ;    ! impersonal forms  juleta...
  +Pers+Prt%+Use%/Rare:%{W%}%+nu NU ;       !  NU-affixes julenud...

LEXICON LEPPIMA   ! VVS 28

                      : LEPPIMA_STRONG ;
                      :%{W%} IMPERS ;         ! impersonal forms
             +Pers+Prs:%{W%}%+ IND_COND_PRS ;    ! 

LEXICON LEPPIMA_STRONG   ! strong grade sub-paradigm

                 +Pers:%+ SUPINE_V ;       ! -ma etc, -v, -vat
                      :%+D7 INFINITIVE ;    ! infinitive, gerund (des-form)
       +Pers+Prs+Imprt:%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem
             +Pers+Prt:%+nu NU ;           !  NU-affixes
         +Pers+Prt+Ind:%+I7S7 IND_PAST ;      ! indicative past


LEXICON LEKKIMA   ! VVS 28 & 27
                  ! like SÜTTIMA, but non-grading forms are less used

                      :%{W%} IMPERS ;    ! impersonal forms
           %+Use%/Rare: IMPERS ;         ! impersonal forms;  non-grading forms are less used
                      : LEPPIMA_STRONG ;
             +Pers+Prs:%{W%}%+ IND_COND_PRS ;    ! 
  +Pers+Prs%+Use%/Rare:%+ IND_COND_PRS ;         ! non-grading forms are less used

LEXICON SÜTTIMA   ! VVS 28 & 27
                  ! like LEKKIMA, but weak grade forms are less used

           %+Use%/Rare:%{W%} IMPERS ;    ! impersonal forms; weak grade forms are less used
                      : IMPERS ;         ! impersonal forms
                      : LEPPIMA_STRONG ;
  +Pers+Prs%+Use%/Rare:%{W%}%+ IND_COND_PRS ;    ! weak grade forms are less used
             +Pers+Prs:%+ IND_COND_PRS ;         ! 

LEXICON HAKKAMA    ! VVS 29

                      :%{W%} IMPERS ;        ! impersonal forms
                 +Pers:%+ SUPINE_V ;            ! -ma etc, -v, -vat
                      :%{W%}%+D7 INFINITIVE ;    ! infinitive, gerund (des-form)
       +Pers+Prs+Imprt:%{W%}%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem
             +Pers+Prt:%{W%}%+nu NU ;           !  NU-affixes
         +Pers+Prt+Ind:%+I7S7 IND_PAST ;           ! indicative past
             +Pers+Prs:%+ IND_COND_PRS ;        ! 

! LEXICON SIPLEMA    ! VVS 30 = HAKKAMA VVS 29


LEXICON ÜTLEMA    ! VVS 30  only ütlema, mõtlema; like SIPLEMA, but some parallel forms
                  ! either t disappears in weak grade, or not

                      :le%{W%} IMPERS ;    ! impersonal forms öeld..
           %+Use%/Rare:tle%{W%} IMPERS ;    ! impersonal forms üteld...
                 +Pers:le%+ SUPINE_V ;    ! -ma etc, -v, -vat
                      :le%{W%}%+D7 INFINITIVE ;    ! infinitive, gerund (des-form) öeld...
           %+Use%/Rare:tle%{W%}%+D7 INFINITIVE ;    ! infinitive, gerund (des-form) üteld...
       +Pers+Prs+Imprt:le%{W%}%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem  öelg...
+Pers+Prs+Imprt%+Use%/Rare:tle%{W%}%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem ütelg...
             +Pers+Prt:le%{W%}%+nu NU ;    !  NU-affixes öeln...
  +Pers+Prt%+Use%/Rare:tle%{W%}%+nu NU ;    !  NU-affixes üteln...
         +Pers+Prt+Ind:le%+I7S7 IND_PAST ;    ! indicative past
             +Pers+Prs:le%+ IND_COND_PRS ;    ! 

LEXICON RABELEMA    ! VVS 31; = SIPLEMA + ELAMA 
                    ! assuming that rabel- is less used than rabele- ...

                       : ELAMA ;
            %+Use%/Rare:%{W%} IMPERS ;    ! rabeldakse impersonal forms
            %+Use%/Rare:%{W%}%+D7 INFINITIVE ;    ! rabeldes infinitive, gerund (des-form)
+Pers+Prs+Imprt%+Use%/Rare:%{W%}%+G7 IMPERATIVE ;    ! rabel-gu/ku, -ge/ke, -gem/kem
   +Pers+Prt%+Use%/Rare:%{W%}%+nu NU ;    ! rabelnu NU-affixes

! VVS 32, 33  naerma, seisma etc 
LEXICON NAERMA

                      : IMPERS ;    ! impersonal forms
                 +Pers:%+ SUPINE_V ;    ! -ma etc, -v, -vat
                      :%+D7 INFINITIVE ;    ! infinitive, gerund (des-form)
       +Pers+Prs+Imprt:%+G7 IMPERATIVE ;    ! -gu/ku, -ge/ke, -gem/kem
             +Pers+Prt:%+nu NU ;    !  NU-affixes
         +Pers+Prt+Ind:%+I7S7 IND_PAST ;    ! indicative past
             +Pers+Prs:a%{W%}%+ IND_COND_PRS ;    ! 

