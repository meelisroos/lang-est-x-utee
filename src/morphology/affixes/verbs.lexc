+V:%+d INFINITIVE ;    ! infinitive, gerund (des-form)
+V+Pers+Prs+Imprt:%{W%}%+g IMPERATIVE ; ! -gu/ku, -ge/ke, -gem/kem
+V+Pers+Prt:%+nu NU ;     !  NU-affixes
+V+Pers+Prt+Ind:%+ IND_PAST1 ; ! indicative past1
+V+Pers+Prt+Ind+Sg3+Aff:%+i GI ; ! sai
+V+Pers+Prs:%{W%}%+ IND_COND_PRS ; ! 

