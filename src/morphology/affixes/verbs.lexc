             +Pers+Prs:u%{W%}%+ IND_COND_PRS ;    ! 

LEXICON IND_COND_PRS    ! indicative, conditional present + imp sg2

            +Imprt+Sg2: GI ;
              +Ind+Neg: GI ;
          +Ind+Sg1+Aff:n GI ;
          +Ind+Sg2+Aff:d GI ;
          +Ind+Sg3+Aff:b GI ;
          +Ind+Pl1+Aff:me GI ;
          +Ind+Pl2+Aff:te GI ;
          +Ind+Pl3+Aff:vad GI ;
                      : CONDITIONAL ;

LEXICON IND_PAST    ! indicative past
                      :i MARKED_PERSON ;
                      : SG3 ;

LEXICON NU    !  NU-affixes

                      : CONDITIONAL ;
              ! +Imprt:d GI ;    ! omitted for now because it would create homonymy
              +Ind+Neg:d GI ;
                  +Prc:d GI ;
                 +Quot:vat GI ;

LEXICON IMPERATIVE    ! imperative

                      :u GI ;    ! elagu
                  +Pl1:em GI ;    ! elagem
                  +Pl2:e GI ;    ! elage

LEXICON SUPINE_V

                  +Sup:ma SUPINE ;
              +Prs+Prc:v GI ;
             +Prs+Quot:vat GI ;

LEXICON SUPINE

                  +Ill: GI ;    ! elama
                  +Ine:s GI ;    ! elamas
                  +Ela:st GI ;
                  +Tra:ks GI ;
                  +Abe:ta GI ;

LEXICON IMPERS    ! impersonal forms

               +Impers:%+T7 IMPERS_DUD ;
               +Impers:%+T7 IMPERS_AKSE ;

LEXICON IMPERS_DUD    ! impersonal forms -[td]ud

             +Prs+Cond:aks GI ;
            +Prs+Imprt:agu GI ;
          +Prs+Ind+Neg:a GI ;
              +Prs+Prc:av GI ;
             +Prs+Quot:avat GI ;
             +Prt+Cond:uks GI ;
          +Prt+Ind+Aff:i GI ;
          +Prt+Ind+Neg:ud GI ;
              +Prt+Prc:ud GI ;
                  +Sup:ama GI ;

LEXICON IMPERS_AKSE    ! impersonal form -[td0]akse

          +Prs+Ind+Aff:akse GI ;

LEXICON INFINITIVE    ! infinitive, gerund (des-form)

                  +Inf:a GI ;
                  +Ger:es GI ;

LEXICON CONDITIONAL    ! conditional present and past  

                 +Cond:ks GI ;
                 +Cond:ksi MARKED_PERSON ;

LEXICON MARKED_PERSON     ! sg3 ~ underspecified is missing from here 
                      : SG1 ;
                      : SG2 ;
                      : PL1 ;
                      : PL2 ;
                      : PL3 ;

LEXICON SG1
              +Sg1+Aff:n GI ;

LEXICON SG2
              +Sg2+Aff:d GI ;

LEXICON SG3
              +Sg3+Aff: GI ;

LEXICON SG3B
              +Sg3+Aff:b GI ;

LEXICON PL1
              +Pl1+Aff:me GI ;

LEXICON PL2
