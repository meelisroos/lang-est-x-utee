+V+Pers:e%+ SUPINE_V ;  ! rabele-ma etc, -v, -vat
+V%+Use%/Rare:%+d INFINITIVE ;    ! rabeldes infinitive, gerund (des-form)
+V:e%+d INFINITIVE ;    ! rabeledes infinitive, gerund (des-form)
+V+Pers+Prs+Imprt%+Use%/Rare:%+g IMPERATIVE ; ! rabel-gu/ku, -ge/ke, -gem/kem
+V+Pers+Prs+Imprt:e%+g IMPERATIVE ; ! rabele-gu/ku, -ge/ke, -gem/kem
+V+Pers+Prt%+Use%/Rare:%+nu NU ;     ! rabelnu NU-affixes
+V+Pers+Prt:e%+nu NU ;     ! rabelenu NU-affixes
+V+Pers+Prt+Ind:e%+s IND_PAST ; ! indicative past
+V+Pers+Prs:e%+ IND_COND_PRS ; ! 

